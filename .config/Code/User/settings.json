    "git.confirmSync": false,
    "settingsSync.ignoredExtensions": [
        "austin.code-gnu-global",
        "hars.cppsnippets",
        "mitaki28.vscode-clang",
        "ms-vscode.cpptools",
        "jeff-hykin.better-cpp-syntax",
        "ms-vscode-remote.remote-wsl",
